    "morgan": "^1.9.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/mongoose": "^5.7.6",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.1",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
